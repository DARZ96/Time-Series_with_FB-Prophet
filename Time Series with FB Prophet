      "cell_type": "code",
      "metadata": {
        "id": "WFii_1-b_xGf"
      },
      "source": [
        "def sales_prediction(Store_ID, sales_df, periods):\n",
        "  # Function that takes in the data frame, storeID, and number of future period forecast\n",
        "  # The function then generates date/sales columns in Prophet format\n",
        "  # The function then makes time series predictions\n",
        "\n",
        "  sales_df = sales_df[ sales_df['Store'] == Store_ID ]\n",
        "  sales_df = sales_df[['Date', 'Sales']].rename(columns = {'Date': 'ds', 'Sales':'y'})\n",
        "  sales_df = sales_df.sort_values('ds')\n",
        "  \n",
        "  model    = Prophet()\n",
        "  model.fit(sales_df)\n",
        "  future   = model.make_future_dataframe(periods=periods)\n",
        "  forecast = model.predict(future)\n",
        "  figure   = model.plot(forecast, xlabel='Date', ylabel='Sales')\n",
        "  figure2  = model.plot_components(forecast)"
      ],
      "execution_count": 47,
      "outputs": []
    },
    {
      "cell_type": "code",
      "metadata": {
        "id": "k2Zv3i-mBbC_",
